  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^13.13.52",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "jest": "^26.6.3",
    "jest-dom": "^3.0.2",
    "np": "^6.5.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@anywhere-ui/core": "^0.1.1"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
