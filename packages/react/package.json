{
  "name": "@anywhere-ui/react",
  "sideEffects": false,
  "version": "0.1.1",
  "description": "React specific wrapper AnywhereUI Web Components library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gjovanovicst/anywhere-ui.git"
  },
  "scripts": {
