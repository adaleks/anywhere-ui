{
  "name": "@anywhere-ui/vue",
  "sideEffects": false,
  "version": "0.1.1",
  "description": "Vue specific wrapper AnywhereUI Web Components",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gjovanovicst/anywhere-ui.git"
  },
  "scripts": {
    "lint": "echo add linter",
    "test": "jest",
    "build": "npm run clean && npm run compile && npm run bundle",
    "bundle": "rollup --config rollup.config.js",
    "clean": "rimraf dist dist-transpiled",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@vue/test-utils": "^2.0.0-rc.6",
    "rimraf": "^2.6.3",
    "rollup": "^2.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.2",
    "vue": "^3.1.1"
  },
  "dependencies": {
    "@anywhere-ui/core": "^0.1.1"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
